<script type="text/javascript">
	function trackOutboundLink(link, category, action) {
	 
		try {
		_gaq.push(['_trackEvent', category , action]);
		} catch(err){}
		 
		setTimeout(function() {
		document.location.href = link.href;
		}, 100);
	}

  $(function () {
    $('.outbound_link').click(function () {
      click_action = this.getAttribute('title');
      click_link = this.getAttribute('href');
      click_category = 'Outbound Links';
      console.log(click_action + " " + click_link + " " + click_category);
      trackOutboundLink(click_link, click_category, click_action);
      return false;
    })
  });
</script>